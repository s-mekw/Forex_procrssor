# Forex Processor Environment Variables Example
# Copy this file to .env and fill in your actual values

# MT5 Settings
FOREX_MT5_LOGIN=your_mt5_login_here
FOREX_MT5_PASSWORD=your_mt5_password_here
FOREX_MT5_SERVER=MetaQuotes-Demo
FOREX_MT5_TIMEOUT=60000

# Database Settings (optional)
FOREX_DB_HOST=localhost
FOREX_DB_PORT=5432
FOREX_DB_NAME=forex_db
FOREX_DB_USER=your_db_user
FOREX_DB_PASSWORD=your_db_password

# Application Settings
FOREX_APP_NAME=forex_processor
FOREX_APP_VERSION=0.1.0
FOREX_DEBUG=false
FOREX_LOG_LEVEL=INFO

# Data Processing Settings
FOREX_BATCH_SIZE=1000
FOREX_MAX_WORKERS=4
FOREX_MEMORY_LIMIT_GB=8.0

# Model Settings
FOREX_MODEL_TYPE=lstm
FOREX_PREDICTION_HORIZON=24
FOREX_CONFIDENCE_THRESHOLD=0.7

# Storage Settings
FOREX_DATA_DIR=data
FOREX_CACHE_DIR=.cache
FOREX_MODEL_DIR=models